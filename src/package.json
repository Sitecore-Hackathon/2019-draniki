    "test": "jest --no-cache --config ./scripts/jest/jest.config.js || exit 0",
    "test-watch": "jest --watch --notify --config ./scripts/jest/jest.config.js",
    "test-cover": "jest --coverage --config ./scripts/jest/jest.config.js"
